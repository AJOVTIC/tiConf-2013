"#row": {
	height: 70,
	selectedBackgroundColor: "#cdcdcd",
	className: "agendaRow"
},

"#title": {
	top: 10,
	left: 10,
	ellipsize: true,
	wordWrap: false,
	color: "#373e47",
	font: {
		fontWeight: "bold",
		fontSize: 14
	}
},

